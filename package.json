	"scripts": {
		"test": "jest",
		"build": "npm run build:client && npm run build:server",
		"build:client": "rm -rf assets/client && rm -rf build/static && NODE_ENV=production NODE_OPTIONS=--max_old_space_size=10240 webpack --config ./webpack/index.client.js",
		"build:server": "rm -rf assets/server && rm -rf build/server && NODE_ENV=production NODE_OPTIONS=--max_old_space_size=10240 webpack --config ./webpack/index.server.js",
		"build:server:dev": "rm -rf assets/server && rm -rf build/server && NODE_OPTIONS=--max_old_space_size=10240 webpack --config ./webpack/index.server.js",
		"build:client:watch": "rm -rf assets/client && rm -rf build/static && NODE_OPTIONS=--max_old_space_size=10240 webpack --watch --config ./webpack/index.client.js",
		"build:server:watch": "rm -rf assets/server && rm -rf build/server && NODE_OPTIONS=--max_old_space_size=10240 webpack --watch --config ./webpack/index.server.js",
		"start:dev": "nodemon build/server/index.js",
		"start:stage": "node build/server/index.js",
		"start": "node build/server/index.js",
		"lint": "eslint . --ext .js,.jsx",
		"lint:fix": "eslint . --fix  --ext .js,.jsx",
		"clean": "rm -rf node_modules && rm -rf assets && rm -rf build",
		"clean:build": "rm -rf assets && rm -rf build",
		"reinstall": "npm run clean && npm install",
		"storybook": "start-storybook"
	},
